<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RootNamespace>Spy</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NothenderEnderEngine" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="DGEFramework">
      <HintPath>Dependencies\DGEFramework.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Include="Dependencies\DGEFramework.pdb" CopyToOutputDirectory="Always" />
  </ItemGroup>
  
</Project>
